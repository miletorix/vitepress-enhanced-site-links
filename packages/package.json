{
  "name": "@miletorix/vitepress-enhanced-site-links",
  "version": "1.0.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/miletorix/vitepress-enhanced-site-links"
  },
  "homepage": "https://miletorix.github.io/vitepress-enhanced-site-links/",
  "description": "Vue components for enhanced link cards and link groups for VitePress.",
  "main": "./dist/enhanced-site-links.umd.js",
  "module": "./dist/enhanced-site-links.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/enhanced-site-links.es.js",
      "require": "./dist/enhanced-site-links.umd.js"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "clean": "rimraf dist node_modules .vite .turbo",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "vitepress",
    "vue",
    "components",
    "card",
    "link",
    "enhanced",
    "ui"
  ],
  "author": "miletorix",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vitepress": "^1.6.3",
    "vue-tsc": "^3.0.0"
  }
}
